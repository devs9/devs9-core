{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-clean-code",
    "tslint-react-hooks",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "eofline": true,
    "typedef": true,
    "import-name": false,
    "prefer-const": true,
    "variable-name": false,
    "function-name": false,
    "switch-default": true,
    "jsdoc-format": false,
    "import-spacing": true,
    "interface-name": false,
    "member-access": false,
    "trailing-comma": false,
    "comment-format": false,
    "newspaper-order": true,
    "ordered-imports": false,
    "jsx-boolean-value": false,
    "no-commented-code": false,
    "no-duplicate-string": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false,
    "react-hooks-nesting": "error",
    "no-else-after-return": false,
    "no-trailing-whitespace": false,
    "no-identical-functions": false,
    "no-implicit-dependencies": false,
    "object-literal-sort-keys": false,
    "no-parameter-reassignment": false,
    "no-consecutive-blank-lines": false,
    "interface-over-type-literal": false,
    "whitespace": [true, "check-decl"],
    "quotemark": [true, "single", "jsx-double"],
    "interface-name-prefix": false,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "semicolon": [true, "never", "ignore-bound-class-methods"],
    "space-before-function-paren": [true, {"anonymous": "always", "named": "never", "asyncArrow": "always"}],
    "max-line-length": [true, {"limit": 125, "ignore-pattern": "^import |^export {(.*?)}"}]
  },
  "jsRules": {}
}
